<%- include('partials/header'); -%>

    <div class = "box" id = "heading">
      <h1>Compose</h1>
    <div class="box">
          <p><%=composeContent%> <br> </p>
    </div>

    <div class="box">
      <form class="item" action="/" method="POST">
        <div class="form-group">
          <label for="Title">Title</label>
          <input name="newTitle" type="text" class="form-control" id="" placeholder="">
          <small id="title" class="form-text text-muted"></small>
        </div>
        <div class="form-group">
          <label for="Post">Post</label>
          <textarea rows = "4" name="newPost" type="text" class="form-control" style="height:150px;" id="" placeholder="" autofocus="on" autocomplete="off"> </textarea>
        </div>
        <button type="submit" name="list" class="btn btn-primary" value=<%=pageName%> >Publish</button>
      </form>
    </div>


<%- include('partials/footer'); -%>
